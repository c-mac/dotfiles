[alias]
    ap = add --patch
    c = commit
    co = checkout
    bl = branch --list
    bd = branch -D
    s = status
    ri = rebase -i
    lg = log --oneline --abbrev-commit --all --graph --decorate --color
    sh = diff-tree --no-commit-id --name-only -r
    diff = diff -w
[push]
    default = current
[core]
    excludesfile = ~/.gitignore
    autocrlf = input
[user]
    name = Cameron McCallie
    email = mccallie.cameron@gmail.com
